{
  "name": "frontend-boilerplate",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "prestart": "rimraf ./dist",
    "start": "gulp pug:dev && gulp css:dev && gulp",
    "prebuild": "rimraf ./production",
    "build": "cross-env NODE_ENV=docs gulp build",
    "browser": "browser-sync start -s 'docs'",
    "lint:html": "gulp pug:lint",
    "script:lint": "eslint --fix --ext .js --ignore-path .gitignore .",
    "style:lint": "stylelint --fix \"**/*.scss\"",
    "lint-staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.scss": [
      "yarn style:lint",
      "git add --force"
    ]
  },
